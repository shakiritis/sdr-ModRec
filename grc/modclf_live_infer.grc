- name: epy_block_1
  id: epy_block
  parameters:
    _source_code: "import numpy as np\nimport onnxruntime as ort\nimport json\nimport\
      \ pmt\nfrom gnuradio import gr\n\nclass blk(gr.sync_block):\n    \"\"\"\n  \
      \  Predict modulation once every update_period_s seconds (time-based),\n   \
      \ using the last win_len samples from a ring buffer.\n    \"\"\"\n\n    def\
      \ __init__(self,\n                 model_path=\"models/modclf.onnx\",\n    \
      \             labels_path=\"models/labels.json\",\n                 win_len=1024,\n\
      \                 samp_rate=1e6,\n                 update_period_s=1.0,\n  \
      \               print_on_change=True):\n        gr.sync_block.__init__(\n  \
      \          self,\n            name=\"modclf_infer_onnx_1hz\",\n            in_sig=[np.complex64],\n\
      \            out_sig=[np.complex64, np.float32],\n        )\n\n        self.message_port_register_out(pmt.intern(\"\
      label_out\"))\n\n        self.win_len = int(win_len)\n        self.samp_rate\
      \ = float(samp_rate)\n        self.update_period_s = float(update_period_s)\n\
      \        self.print_on_change = bool(print_on_change)\n\n        self.samples_per_update\
      \ = max(1, int(round(self.samp_rate * self.update_period_s)))\n        self.samples_since_update\
      \ = 0\n\n        with open(labels_path, \"r\") as f:\n            self.labels\
      \ = json.load(f)\n\n        sess_opts = ort.SessionOptions()\n        sess_opts.intra_op_num_threads\
      \ = 1\n        self.sess = ort.InferenceSession(model_path, sess_options=sess_opts,\
      \ providers=[\"CPUExecutionProvider\"])\n        self.input_name = self.sess.get_inputs()[0].name\n\
      \n        self.buf = np.zeros(self.win_len, dtype=np.complex64)\n        self.widx\
      \ = 0\n        self.filled = 0\n\n        self.current_pred = 0.0\n        self.last_pred\
      \ = -1\n\n    def _preprocess(self, x_cplx):\n        re = np.real(x_cplx).astype(np.float64)\n\
      \        im = np.imag(x_cplx).astype(np.float64)\n        p = np.mean(re*re\
      \ + im*im)\n        if (not np.isfinite(p)) or p < 1e-18:\n            return\
      \ None\n        x = x_cplx / np.float32(np.sqrt(p))\n\n        I = np.real(x).astype(np.float32)\n\
      \        Q = np.imag(x).astype(np.float32)\n        X = np.stack([I, Q], axis=0)[None,\
      \ :, :]  # (1,2,L)\n        return X\n\n    def _infer_from_buffer(self):\n\
      \        if self.filled < self.win_len:\n            return None\n\n       \
      \ # last win_len samples in time order\n        if self.widx == 0:\n       \
      \     window = self.buf.copy()\n        else:\n            window = np.concatenate((self.buf[self.widx:],\
      \ self.buf[:self.widx])).astype(np.complex64, copy=False)\n\n        X = self._preprocess(window)\n\
      \        if X is None:\n            return None\n\n        logits = self.sess.run(None,\
      \ {self.input_name: X})[0]  # (1,C)\n        return int(np.argmax(logits, axis=1)[0])\n\
      \n    def _write_ring(self, x):\n        n = len(x)\n        if n == 0:\n  \
      \          return\n\n        # IMPORTANT FIX: if GNU Radio gives us >= win_len\
      \ samples in one call,\n        # just keep the last win_len samples.\n    \
      \    if n >= self.win_len:\n            tail = x[-self.win_len:].astype(np.complex64,\
      \ copy=False)\n            self.buf[:] = tail\n            self.widx = 0\n \
      \           self.filled = self.win_len\n            return\n\n        end =\
      \ self.widx + n\n        if end <= self.win_len:\n            self.buf[self.widx:end]\
      \ = x\n        else:\n            first = self.win_len - self.widx\n       \
      \     self.buf[self.widx:] = x[:first]\n            second = end % self.win_len\n\
      \            if second > 0:\n                self.buf[:second] = x[first:first+second]\n\
      \n        self.widx = end % self.win_len\n        self.filled = min(self.win_len,\
      \ self.filled + n)\n\n    def work(self, input_items, output_items):\n     \
      \   x = input_items[0]\n        y = output_items[0]\n        ylab = output_items[1]\n\
      \n        y[:] = x\n        ylab[:] = self.current_pred\n\n        self._write_ring(x)\n\
      \        self.samples_since_update += len(x)\n\n        # run at most once per\
      \ work() call (keeps GUI smooth)\n        if self.filled >= self.win_len and\
      \ self.samples_since_update >= self.samples_per_update:\n            pred =\
      \ self._infer_from_buffer()\n            if pred is not None:\n            \
      \    self.current_pred = float(pred)\n                if pred != self.last_pred:\n\
      \                    label = self.labels[pred]\n                    if self.print_on_change:\n\
      \                        print(\"[modclf] predicted:\", label)\n           \
      \         self.message_port_pub(pmt.intern(\"label_out\"), pmt.intern(label))\n\
      \                    self.last_pred = pred\n\n            self.samples_since_update\
      \ -= self.samples_per_update\n\n        return len(x)\n\n"
    affinity: ''
    alias: ''
    comment: ''
    labels_path: '"/media/shakir/EXT4_1TB/projects/sdr/modclf-gnuradio-ml/models/labels.json"'
    maxoutbuf: '0'
    minoutbuf: '0'
    model_path: '"/media/shakir/EXT4_1TB/projects/sdr/modclf-gnuradio-ml/models/modclf.onnx"'
    print_on_change: 'True'
    samp_rate: samp_rate
    update_period_s: '1.0'
    win_len: '1024'
  states:
    _io_cache: ('modclf_infer_onnx_1hz', 'blk', [('model_path', "'models/modclf.onnx'"),
      ('labels_path', "'models/labels.json'"), ('win_len', '1024'), ('samp_rate',
      '1000000.0'), ('update_period_s', '1.0'), ('print_on_change', 'True')], [('0',
      'complex', 1)], [('0', 'complex', 1), ('1', 'float', 1), ('label_out', 'message',
      1)], '\n    Predict modulation once every update_period_s seconds (time-based),\n    using
      the last win_len samples from a ring buffer.\n    ', ['print_on_change', 'samp_rate',
      'update_period_s', 'win_len'])
    bus_sink: false
    bus_source: false
    bus_structure: null
    coordinate: [1120, 1488.0]
